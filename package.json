{
   "name": "gulp",
   "version": "1.0.0",
   "description": "Gulp starter template",
   "author": "Nikita Suchkov",
   "license": "MIT",
   "browserslist": "> 0.5%, last 3 versions, not dead",
   "repository": "https://github.com/suchkovcode/gulp",
   "private": true,
   "scripts": {
      "start": "gulp",
      "build:dev": "gulp dev",
      "build:prod": "gulp pub",
      "build:zip": "gulp zip",

      "test": "gulp test",
      "test:check": "prettier --check src/**/* && eslint && stylelint src/**/*.scss",
      "test:fix": "prettier --write . && eslint --fix && stylelint src/**/*.scss --fix",
      "server": "gulp server"
   },
   "lint-staged": {
      "*.js": ["prettier --write", "eslint --fix --color"],
      "*.scss": "stylelint --syntax=scss --fix"
   },
   "devDependencies": {
      "webpack-stream": "^7.0.0",
      "imports-loader": "^5.0.0",
      "babel-loader": "^9.1.3",
      "husky": "^9.0.10",
      "lint-staged": "^15.2.2",

      "gulp": "^4.0.2",
      "gulp-autoprefixer": "^9.0.0",
      "gulp-clean": "^0.4.0",
      "gulp-csso": "^4.0.1",
      "gulp-eslint": "^6.0.0",
      "gulp-file-include": "^2.3.0",
      "gulp-group-css-media-queries": "^1.2.2",
      "gulp-htmlhint": "^4.0.2",
      "gulp-htmlmin": "^5.0.1",
      "gulp-imagemin": "^8.0.0",
      "gulp-load-plugins": "^2.0.8",
      "gulp-notify": "^4.0.0",
      "gulp-purgecss": "^5.0.0",
      "gulp-rename": "^2.0.0",
      "gulp-sass": "^5.1.0",
      "gulp-shorthand": "^1.1.0",
      "gulp-size": "^5.0.0",
      "gulp-sourcemaps": "^3.0.0",
      "gulp-webp": "^5.0.0",
      "gulp-zip": "^6.0.0",

      "@babel/cli": "^7.23.9",
      "@babel/core": "^7.23.9",
      "@babel/preset-env": "^7.23.9",

      "eslint": "^8.56.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-import": "^2.29.1",
      "eslint-plugin-prettier": "^5.1.3",

      "stylelint": "^16.2.1",
      "stylelint-config-prettier-scss": "^1.0.0",
      "stylelint-config-standard-scss": "^13.0.0",
      "stylelint-order": "^6.0.4",

      "prettier": "^3.2.5",
      "browser-sync": "^3.0.2",
      "require-dir": "^1.2.0",
      "sass": "^1.70.0"
   }
}

{
   "printWidth": 150,
   "tabWidth": 3,
   "useTabs": false,
   "semi": true,
   "singleQuote": false,
   "quoteProps": "consistent",
   "trailingComma": "all",
   "bracketSpacing": true,
   "bracketSameLine": true,
   "arrowParens": "always",
   "rangeStart": 0,
   "requirePragma": false,
   "insertPragma": false,
   "proseWrap": "always",
   "htmlWhitespaceSensitivity": "css",
   "vueIndentScriptAndStyle": false,
   "endOfLine": "lf",
   "embeddedLanguageFormatting": "auto",
   "singleAttributePerLine": false
}

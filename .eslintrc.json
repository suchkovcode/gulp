{
   "env": {
      "browser": true,
      "es2021": true,
      "jquery": true
   },
   "extends": "airbnb-base",
   "overrides": [],
   "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
         "jsx": true
     }
   },
   "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "double"]
   }
}
